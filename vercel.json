{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/images/(.+)",
        "dest": "public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }